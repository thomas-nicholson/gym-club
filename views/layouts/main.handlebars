<body class="font">
    <header class="homeheader">
        <a href="/"><img class="logo" src="/Images/GymClub.png"></a>
        <input type="search" id="search-bar" placeholder="Search..."/>
        <div class="menuflex">
        
              <a class="navBtn homeNav font" href="/">Home</a>
              {{#if user_id}}
              <a class="navBtn" href="/user/{{user_id}}">Profile</a>
              {{/if}}
              {{#if logged_in}}
              <a class="navBtn" id="logout-button" href="/">Logout</a>
